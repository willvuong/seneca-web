<div class="container">
    <div class="row">
        <div class="col-md-12">

            <h2>{{User.userName}} ( {{User.email}} ) History</h2>
            <hr />

            <div class="table-responsive">
                <table class="table ">
                    <thead>
                        <tr>
                            <th>Login Date/Time</th>
                            <th>Client Information</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each User.loginHistory}}
                        <tr>
                            <td>{{User.loginHistory.dateTime}}</td>
                            <td>{{User.loginHistory.userAgent}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>